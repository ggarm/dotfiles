#!/usr/bin/env python

fg = '\033[38;5;'
bg = '\033[48;5;'

cStrings=[]

for i in range( 0, 256):
	n = str(i)
	fgstr = fg + n + 'm' + n + ' '
	bgstr = bg + n + 'm' 'XXX'
	cStrings.append(fgstr + bgstr + '\033[0m')

for i in range( 0, 10):
	cStrings[i] = '  ' + cStrings[i]

for i in range( 10, 100):
	cStrings[i] = ' ' + cStrings[i]

x = 0
while x <= 25:
	print ' '.join(cStrings[x:256:26])
	x = x + 1
